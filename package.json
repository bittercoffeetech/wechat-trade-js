{
  "name": "wechat-trade",
  "version": "0.1.0",
  "description": "Wechat Trade Client",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "compile": "rm -Rf dist/* & tsc",
    "test": "tsc & jest",
    "tcover": "tsc & jest --coverage",
    "docs": "npx typedoc"
  },
  "keywords": [
    "Wechat",
    "API"
  ],
  "author": "BitterCoffee",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "class-transformer": "^0.2.3",
    "crypto-js": "^4.0.0",
    "csvtojson": "^2.0.10",
    "fast-xml-parser": "^3.17.3",
    "he": "^1.2.0",
    "moment": "^2.26.0",
    "nanoid": "^3.1.9",
    "nconf": "^0.10.0",
    "reflect-metadata": "^0.1.13",
    "typescriptcollectionsframework": "^4.4.13"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/crypto-js": "^3.1.47",
    "@types/he": "^1.1.1",
    "@types/jest": "^25.2.3",
    "@types/moment": "^2.13.0",
    "@types/nconf": "^0.10.0",
    "@types/node": "^14.0.4",
    "@types/reflect-metadata": "^0.1.0",
    "@types/rewire": "^2.5.28",
    "@types/utf8": "^2.1.6",
    "jest": "^26.0.1",
    "rewire": "^5.0.0",
    "ts-jest": "^26.1.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.5"
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/*-test.ts"
    ],
    "testTimeout": 10000,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "isolatedModules": true
      }
    },
    "modulePaths": [
      "<rootDir>/"
    ]
  }
}
